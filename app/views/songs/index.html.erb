<h1>Songs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Release Year</th>
    <th>Artist</th>
    <th>Genre</th>
  </tr>
  <tbody>
    <% @songs.each do |s| %>
    <tr>
      <td><%=s.title%></td>
      <td>
        <%= "Released in #{s.release_year}" if s.released %>
      </td>
      <td><%=s.artist_name%></td>
      <td><%=s.genre%></td>
    </tr>
    <%end%>
  </tbody>
</table>
